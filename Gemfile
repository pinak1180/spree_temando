source "http://rubygems.org"

# Declare your gem's dependencies in spree_temando.gemspec.
# Bundler will treat runtime dependencies like base dependencies, and
# development dependencies will be added by default to the :development group.
gemspec

# These gems are not actual dependencies, but tools that are optionally used in development.
# For this reason, we put them in the Gemfile, rather than the gemspec.
group :development do
  gem 'pry'
  gem 'pry-remote'
end
